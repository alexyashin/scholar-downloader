{
	"name": "tylz",
	"description": "Google scholar downloader",
	"version": "1",
	"permissions": [
		"activeTab",
        "downloads",
        "tabs",
        "<all_urls>"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
        "default_icon": "start-icon.png",
		"default_title": "Google scholar downloader"
	},
	"manifest_version": 2
}